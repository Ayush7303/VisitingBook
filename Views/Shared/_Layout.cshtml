                <i class="nav-icon fa-solid fa-chart-line"></i>
                <p>
                  Dashboard
                </p>
              </a>
            </li>
            <li class="nav-item">
              <a href="@Url.ActionLink("Index","Home")" class="nav-link">
                <i class="nav-icon fa-solid fa-house"></i>
                <p>
                  Home
                  <i class="right fas fa-angle-left"></i>
                </p>
